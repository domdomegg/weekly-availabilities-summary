  },
  "dependencies": {
    "weekly-availabilities": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "eslint-config-domdomegg": "^1.2.3",
    "shx": "^0.3.4",
    "tsconfig-domdomegg": "^1.0.0",
    "typescript": "^5.4.4",
    "vitest": "^1.6.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-domdomegg"
    ]
  }
}
